Extension { #name : 'SpPresenter' }

{ #category : '*myBecher-WindowProfile' }
SpPresenter >> initialPosition [

	^ position
]

{ #category : '*myBecher-WindowProfile' }
SpPresenter >> position [

	^ position
]

{ #category : '*myBecher-WindowProfile' }
SpPresenter >> position: aPoint [

	position := aPoint
]
